components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"Script"}},{"id":19,"class":"sNetworkComponent","data":{}},{"id":40000,"class":"sScriptHelperComponent","data":{}}]},{"class":"cModuleScriptObject","name":"HairModule","guid":[766390908,262424231,3010902222,2990653756],"parentGuid":[1374138560,3022407613,3219806122,350961622],"components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"HairModule"}},{"id":19,"class":"sNetworkComponent","data":{}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":10,"class":"sLuaComponent","data":{"m_luaContent":"local module = {}\r\nlocal UTIL = require(UtilityModule)\r\nlocal SvenUI = world.SvenUI\r\n\r\n--\r\nlocal hairGrivaty = 0.02\r\nlocal hairWeight = 1 --kg\r\nlocal airResistance = 0.2\r\nlocal elasticK = 3\r\nlocal defaultDistance = HairSize.Value + 0.00\r\nlocal elasticDualK = 1\r\nlocal defaultDualDistance = HairSize.Value * 1.5\r\nlocal elasticClothK = 0.01\r\nlocal maxDragDistance = HairSize.Value * 3\r\n\r\nlocal isInitFinished = false\r\n\r\nlocal hairRootTbl = {}\r\nlocal hairPartical = {}\r\nlocal hairParticalLastSpeed = {}\r\nlocal hairParticalSpeed = {}\r\nlocal hairParticalLastPosition = {}\r\n\r\nlocal _deltaTime = 1/60;\r\n\r\nfunction module:RegistHair(index)\r\n\tprint('regist success: ', index)\r\nend\r\n\r\nfunction module:RegistHairRoot(root, index)\r\n\tif hairRootTbl[index] == nil then\r\n\t\thairRootTbl[index] = root\r\n\telse\r\n\t\t--error(\"RegistHair Error!\")\r\n\tend\r\nend\r\n\r\nfunction module:RegistHairPartical(hair, rootIndex, index)\r\n\tif hairPartical[rootIndex] == nil then\r\n\t\thairPartical[rootIndex] = {}\r\n\tend\r\n\t\r\n\thairPartical[rootIndex][index] = hair;\r\nend\r\n\r\nfunction module:InitDone()\r\n\tisInitFinished = true\r\n\t\r\n\t--record position\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\r\n\t\thairParticalLastSpeed[hairRootIndex] = {}\r\n\t\thairParticalSpeed[hairRootIndex] = {}\r\n\t\thairParticalLastPosition[hairRootIndex] = {}\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\thairParticalLastSpeed[hairRootIndex][hairIndex] = Vector3(0, 0, 0)\t\t\t\r\n\t\t\thairParticalSpeed[hairRootIndex][hairIndex] = Vector3(0, 0, 0)\t\r\n\t\t\thairParticalLastPosition[hairRootIndex][hairIndex] = Vector3(0, 0, 0)\t\r\n\t\tend\r\n\tend\r\n\t\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\t\t\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\thairParticalLastPosition[hairRootIndex][hairIndex] = hairPartical[hairRootIndex][hairIndex].Position\t\t\r\n\t\tend\r\n\tend\r\nend\r\n\r\nfunction module:OnLogicUpdate(deltaTime)\r\n\tif isInitFinished == false then\r\n\t\treturn\r\n\tend\r\n\t_deltaTime = deltaTime\r\n\t\r\n\t--calculate grivaty\r\n\tmodule:CalculateGrivaty(hairGrivaty)\r\n\t\r\n\t--calculate wind\r\n\tif Wind.Value ~= Vector3(0,0,0) then\r\n\t\t--wind force\r\n\t\t--module:CalculateWind()\r\n\tend\r\n\t\r\n\t--elastic constraint\r\n\tmodule:CalculateElasticConstraint()\r\n\tmodule:CalculateDualElasticConstraint()\r\n\t--module:CalculateClothConstraint()\r\n\t\r\n\tmodule:CalculateDrag()\r\n\t\r\n\tmodule:ClampSpeed()\r\n\t--set position\r\n\tmodule:SetPosition()\r\n\t\r\n\t--debug\r\n\tif (hairRootIndex == 1 and hairIndex == 1) then\r\n\t\t--print(\"newV:\", newV.y)\r\n\tend\r\nend\r\n\r\nfunction module:CalculateGrivaty(hairGrivaty)\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\t\t\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\tlocal grivatyASpeed = hairGrivaty -- a = g \r\n\t\t\t--update speed\r\n\t\t\thairParticalSpeed[hairRootIndex][hairIndex] = hairParticalSpeed[hairRootIndex][hairIndex] + Vector3(0, -grivatyASpeed, 0)\t\t\r\n\t\tend\r\n\tend\r\nend\r\n\r\nfunction module:CalculateWind()\r\n\tlocal randomWind = true\r\n\tlocal randomRate = 80\r\n\t\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\t\t\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\tif randomWind and math.random(0, 100) < randomRate then\t\t\t\r\n\t\t\t\tlocal force = UTIL.GetVector3Length(Wind.Value)\r\n\t\t\t\tlocal aSpeed = force / hairWeight\r\n\t\t\t\tlocal sppedAddOn = aSpeed * Wind.Value.Normalized\r\n\t\t\t\t\r\n\t\t\t\thairParticalSpeed[hairRootIndex][hairIndex] = hairParticalSpeed[hairRootIndex][hairIndex] + sppedAddOn\r\n\t\t\tend\r\n\t\tend\r\n\tend\r\nend\r\n\r\nfunction module:CalculateElasticConstraint()\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\t\t\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\tlocal previousPartical\r\n\t\t\tif hairIndex == 1 then\r\n\t\t\t\tpreviousPartical = hairRootTbl[hairRootIndex]\r\n\t\t\telse\r\n\t\t\t\tpreviousPartical = hairPartical[hairRootIndex][hairIndex - 1]\r\n\t\t\tend\r\n\r\n\t\t\tlocal directionToPartical = previousPartical.Position - hair.Position\r\n\t\t\tif directionToPartical.x == 0 and directionToPartical.y == 0 and directionToPartical.z == 0 then\r\n\t\t\telse\r\n\t\t\t\tdirectionToPartical = directionToPartical.Normalized\r\n\t\t\tend\r\n\t\t\t\r\n\t\t\tlocal elasticCenterPosition = previousPartical.Position - directionToPartical * defaultDistance\r\n\r\n\t\t\t--calculate elastic force. If the force is negative, need push hair partical away\r\n\t\t\tlocal distance = elasticCenterPosition - hair.Position\r\n\t\t\tdistance = UTIL.GetVector3Length(distance)\r\n\t\t\tlocal direction = elasticCenterPosition - hair.Position\r\n\t\t\t\r\n\t\t\tif direction.x == 0 and direction.y == 0 and direction.z == 0 then\r\n\t\t\telse\r\n\t\t\t\tdirection = direction.Normalized\r\n\t\t\tend\r\n\t\t\t\r\n\t\t\tif (distance > maxDragDistance) then\r\n\t\t\t\t--set position directly\r\n\t\t\t\thair.Position = previousPartical.Position - direction * maxDragDistance\r\n\t\t\t\tdistance = maxDragDistance\r\n\t\t\tend\r\n\r\n\r\n\t\t\t--update speed\t\t\t\r\n\t\t\tlocal force = distance * elasticK\r\n\t\t\tlocal aSpeed = (force / hairWeight)  -- a=f/m\r\n\t\t\t--if partical is go away from last partical, add a force toward hair root.\r\n\t\t\tlocal speedAddOn = aSpeed * direction\r\n\t\t\tUTIL.Vector3Clamp(speedAddOn, 0.5)\r\n\t\t\thairParticalSpeed[hairRootIndex][hairIndex]\t= hairParticalSpeed[hairRootIndex][hairIndex] + speedAddOn\r\n\t\tend\r\n\tend\r\nend\r\n\r\nfunction module:CalculateDualElasticConstraint()\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\t\t\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\tlocal previousPartical\r\n\t\t\tif hairIndex == 1 then\r\n\t\t\t\tbreak\r\n\t\t\telseif hairIndex == 2 then\r\n\t\t\t\tpreviousPartical = hairRootTbl[hairRootIndex]\r\n\t\t\telse\r\n\t\t\t\tpreviousPartical = hairPartical[hairRootIndex][hairIndex - 2]\r\n\t\t\tend\r\n\r\n\t\t\tlocal directionToPartical = previousPartical.Position - hair.Position\r\n\t\t\tif directionToPartical.x == 0 and directionToPartical.y == 0 and directionToPartical.z == 0 then\r\n\t\t\telse\r\n\t\t\t\tdirectionToPartical = directionToPartical.Normalized\r\n\t\t\tend\r\n\t\t\t\r\n\t\t\tlocal elasticCenterPosition = previousPartical.Position - directionToPartical * defaultDistance\r\n\r\n\t\t\t--calculate elastic force. If the force is negative, need push hair partical away\r\n\t\t\tlocal distance = elasticCenterPosition - hair.Position\r\n\t\t\tdistance = UTIL.GetVector3Length(distance)\r\n\t\t\tlocal force = distance * elasticDualK\r\n\t\t\tlocal aSpeed = (force / hairWeight)  -- a=f/m\r\n\t\t\tlocal direction = elasticCenterPosition - hair.Position\r\n\t\t\t\r\n\t\t\tif direction.x == 0 and direction.y == 0 and direction.z == 0 then\r\n\t\t\telse\r\n\t\t\t\tdirection = direction.Normalized\r\n\t\t\tend\r\n\r\n\t\t\t--update speed\r\n\t\t\tlocal speedAddOn = aSpeed * direction\r\n\t\t\thairParticalSpeed[hairRootIndex][hairIndex]\t= hairParticalSpeed[hairRootIndex][hairIndex] + speedAddOn\r\n\t\tend\r\n\tend\r\nend\r\n\r\nfunction module:CalculateClothConstraint()\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\t\t\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\tlocal leftPartical, rightPartical\r\n\t\t\t\r\n\t\t\t--left\r\n\t\t\tif hairRootIndex ~= 1 and hairRootIndex ~= 2 then\t\t\t\t\r\n\t\t\t\tleftPartical = hairPartical[hairRootIndex - 2][hairIndex]\r\n\t\t\t\tlocal directionToPartical = leftPartical.Position - hair.Position\r\n\t\t\t\tif directionToPartical.x == 0 and directionToPartical.y == 0 and directionToPartical.z == 0 then\r\n\t\t\t\telse\r\n\t\t\t\t\tdirectionToPartical = directionToPartical.Normalized\r\n\t\t\t\tend\r\n\t\t\t\t\r\n\t\t\t\tlocal elasticCenterPosition = leftPartical.Position - directionToPartical * HairSize.Value\r\n\r\n\t\t\t\t--calculate elastic force. If the force is negative, need push hair partical away\r\n\t\t\t\tlocal distance = elasticCenterPosition - hair.Position\r\n\t\t\t\tdistance = UTIL.GetVector3Length(distance)\r\n\t\t\t\tlocal force = distance * elasticClothK\r\n\t\t\t\tlocal aSpeed = (force / hairWeight)  -- a=f/m\r\n\t\t\t\tlocal direction = elasticCenterPosition - hair.Position\r\n\t\t\t\t\r\n\t\t\t\tif direction.x == 0 and direction.y == 0 and direction.z == 0 then\r\n\t\t\t\telse\r\n\t\t\t\t\tdirection = direction.Normalized\r\n\t\t\t\tend\r\n\r\n\t\t\t\t--update speed\r\n\t\t\t\tlocal speedAddOn = aSpeed * direction\r\n\t\t\t\thairParticalSpeed[hairRootIndex][hairIndex]\t= hairParticalSpeed[hairRootIndex][hairIndex] + speedAddOn\r\n\t\t\tend\r\n\t\t\t\r\n\t\t\t--right\r\n\t\t\tif hairRootIndex ~= #hairPartical and hairRootIndex ~= #hairPartical - 1 then\r\n\t\t\t\trightPartical = hairPartical[hairRootIndex + 2][hairIndex]\r\n\t\t\t\tlocal directionToPartical = rightPartical.Position - hair.Position\r\n\t\t\t\tif directionToPartical.x == 0 and directionToPartical.y == 0 and directionToPartical.z == 0 then\r\n\t\t\t\telse\r\n\t\t\t\t\tdirectionToPartical = directionToPartical.Normalized\r\n\t\t\t\tend\r\n\t\t\t\t\r\n\t\t\t\tlocal elasticCenterPosition = rightPartical.Position - directionToPartical * HairSize.Value\r\n\r\n\t\t\t\t--calculate elastic force. If the force is negative, need push hair partical away\r\n\t\t\t\tlocal distance = elasticCenterPosition - hair.Position\r\n\t\t\t\tdistance = UTIL.GetVector3Length(distance)\r\n\t\t\t\tlocal force = distance * elasticK\r\n\t\t\t\tlocal aSpeed = (force / hairWeight)  -- a=f/m\r\n\t\t\t\tlocal direction = elasticCenterPosition - hair.Position\r\n\t\t\t\t\r\n\t\t\t\tif direction.x == 0 and direction.y == 0 and direction.z == 0 then\r\n\t\t\t\telse\r\n\t\t\t\t\tdirection = direction.Normalized\r\n\t\t\t\tend\r\n\r\n\t\t\t\t--update speed\r\n\t\t\t\tlocal speedAddOn = aSpeed * direction\r\n\t\t\t\thairParticalSpeed[hairRootIndex][hairIndex]\t= hairParticalSpeed[hairRootIndex][hairIndex] + speedAddOn\r\n\t\t\tend\r\n\t\tend\r\n\tend\r\nend\r\n\r\nfunction module:CalculateDrag()\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\tlocal lastFramePosition = hairParticalLastPosition[hairRootIndex][hairIndex]\r\n\t\t\tlocal deltaPositionWithLastFrame = lastFramePosition - hair.Position\r\n\t\t\t\r\n\t\t\t--if deltaPosition is larger then maxDistance, set its positon. if not use addon speed\r\n\t\t\tlocal distance = UTIL.GetVector3Length(deltaPositionWithLastFrame)\r\n\r\n\t\t\tif (distance > maxDragDistance) then\r\n\t\t\t\tlocal newDeltaPosition = deltaPositionWithLastFrame.Normalized * distance/1.1\r\n\t\t\t\thair.Position = lastFramePosition - newDeltaPosition\r\n\t\t\telse\r\n\t\t\tend\r\n\t\tend\r\n\tend\r\nend\r\n\r\nfunction module:ClampSpeed()\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\tlocal speed = hairParticalSpeed[hairRootIndex][hairIndex]\r\n\t\t\tlocal lastSpeed = hairParticalLastSpeed[hairRootIndex][hairIndex]\r\n\t\t\t\r\n\t\t\tUTIL.Vector3Clamp(speed, 1)\r\n\t\r\n\t\t\t--air resistance\r\n\t\t\tif (hairRootIndex == 1 and hairIndex == 1) then\r\n\t\t\t\t--print(speed, speed * (1 - airResistance))\r\n\t\t\tend\r\n\t\t\tspeed = speed * (1 - airResistance)\r\n\t\t\t--speed = speed * 0.99\r\n\t\t\t\r\n\t\t\t-- save clamped speed\r\n\t\t\thairParticalSpeed[hairRootIndex][hairIndex] = speed\t\t\r\n\t\tend\r\n\tend\r\nend\r\n\r\nfunction module:SetPosition()\r\n\tfor hairRootIndex, hairRoot in pairs(hairPartical) do\r\n\t\tfor hairIndex, hair in pairs(hairRoot) do\r\n\t\t\tlocal speed = hairParticalSpeed[hairRootIndex][hairIndex]\r\n\t\t\tlocal lastSpeed = hairParticalLastSpeed[hairRootIndex][hairIndex]\r\n\t\t\t\t\t\t\r\n\t\t\tif (UTIL.GetVector3Length(speed) - 0.005) < 0 then\r\n\t\t\t\thairParticalSpeed[hairRootIndex][hairIndex] = Vector3(0, 0, 0)\r\n\t\t\telse\r\n\t\t\t\thair.Position = hair.Position + (lastSpeed + speed) / 2 * _deltaTime\r\n\t\t\tend\r\n\t\t\t\r\n\t\t\thairParticalLastSpeed[hairRootIndex][hairIndex] = speed\r\n\t\t\thairParticalLastPosition[hairRootIndex][hairIndex] = hair.Position\t\t\t\r\n\t\tend\r\n\tend\r\nend\r\n\r\nreturn module","m_scriptType":"kModuleScript"}}]},{"class":"cModuleScriptObject","name":"TimeModule","guid":[1187475720,544886162,2774232755,1236805847],"parentGuid":[1374138560,3022407613,3219806122,350961622],"components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"TimeModule"}},{"id":19,"class":"sNetworkComponent","data":{}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":10,"class":"sLuaComponent","data":{"m_luaContent":"local module = {}\r\n\r\nlocal _time = 0\r\n\r\nfunction module:GetTimeInMS()\r\n\treturn _time\r\nend\r\n\r\nfunction module:AddTime(deltaMSTime)\r\n\t_time = _time + deltaTime * 1000\r\nend\r\n\r\n\r\nreturn module","m_scriptType":"kModuleScript"}}]},{"class":"cModuleScriptObject","name":"UtilityModule","guid":[2599268747,3422568644,2279128290,1888537580],"parentGuid":[1374138560,3022407613,3219806122,350961622],"components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"UtilityModule"}},{"id":19,"class":"sNetworkComponent","data":{}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":10,"class":"sLuaComponent","data":{"m_luaContent":"local module = {}\r\n\r\nfunction module.GetVector3Length(v3)\r\n\treturn math.sqrt(v3.x * v3.x + v3.y * v3.y + v3.z * v3.z)\r\nend\r\n\r\nfunction module.Vector3Clamp(v3, f)\t\r\n\tif v3.x > f then\r\n\t\tv3.x = f\r\n\telseif v3.x < -f then\r\n\t\tv3.x = -f\r\n\tend\r\n\t\r\n\tif v3.y > f then\r\n\t\tv3.y = f\r\n\telseif v3.y < -f then\r\n\t\tv3.y = -f\r\n\tend\r\n\t\r\n\tif v3.z > f then\r\n\t\tv3.z = f\r\n\telseif v3.z < -f then\r\n\t\tv3.z = -f\r\n\tend\r\nend\r\n\r\nreturn module","m_scriptType":"kModuleScript"}}]},{"class":"cStartFunctionObject","name":"Init","guid":[2162176153,1211449886,3150531023,3665052334],"parentGuid":[1374138560,3022407613,3219806122,350961622],"components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"Init"}},{"id":19,"class":"sNetworkComponent","data":{}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":10,"class":"sLuaComponent","data":{"m_luaContent":"local hairModule = require(HairModule)\r\n\r\nlocal hairParticalSize = HairSize.Value\r\nlocal smallValue = 0.01\r\nlocal fWaitTime = 1/60\r\n\r\n--[[\r\nfor iCount = 1, HairCount.Value, 1 do\r\n\tlocal sphere = world:CreateObject('Sphere', \"HairRoot-\" .. iCount, world.Hair.HairRoot, Vector3(0,0,0))\r\n\tsphere.LocalPosition = Vector3(iCount * hairParticalSize * 2,0,0)\r\n\tsphere.Size = Vector3(hairParticalSize, hairParticalSize, hairParticalSize)\r\n\t\r\n\thairModule:RegistHairRoot(sphere, iCount)\r\n\t\t\r\n\t--generate a hair\r\n\tfor i = 1, HairLength.Value, 1 do\r\n\t\tlocal hairPartical = world:CreateObject('Sphere', iCount .. \"-\" .. i, sphere, Vector3(0,0,0))\r\n\t\thairPartical.LocalPosition = Vector3(0, -(hairParticalSize + smallValue) * i,0)\r\n\t\thairPartical.Size = Vector3(hairParticalSize, hairParticalSize, hairParticalSize)\r\n\t\t\r\n\t\t--regist to hair module\r\n\t\thairModule:RegistHairPartical(hairPartical,iCount, i)\r\n\tend\r\n\r\n\twait(fWaitTime)\r\nend\r\n\r\nhairModule:InitDone()\r\n--]]\r\n\r\nwait(0.5)\r\nlocal rootIndex = 1\r\nlocal hairParticalRoot = localPlayer.Avatar.Bone_Head.HairRoot\r\nwhile(true) do\r\n\tlocal rootName = \"Root\" .. rootIndex\r\n\tlocal root = hairParticalRoot[rootName]\r\n\r\n\tif root and root.ActiveInHierarchy then\r\n\t\thairModule:RegistHairRoot(root, rootIndex)\r\n\t\t\r\n\t\t--generate a hair\r\n\t\tfor i = 1, HairLength.Value, 1 do\r\n\t\t\tlocal hairPartical = world:CreateObject('Sphere', rootIndex .. \"-\" .. i, hairParticalRoot, Vector3(0,0,0))\r\n\t\t\thairPartical.LocalPosition = root.LocalPosition + Vector3(0, -(hairParticalSize + smallValue) * i,0)\r\n\t\t\thairPartical.Size = Vector3(hairParticalSize, hairParticalSize, hairParticalSize)\r\n\t\t\thairPartical.Color = Color(0,0,0,255)\r\n\t\t\t\r\n\t\t\t--regist to hair module\r\n\t\t\thairModule:RegistHairPartical(hairPartical, rootIndex, i)\r\n\t\tend\r\n\r\n\t\trootIndex = rootIndex + 1\r\n\t\twait(fWaitTime)\r\n\telse\r\n\t\tbreak\r\n\tend\r\nend\r\n\r\nhairModule:InitDone()","m_scriptType":"kStartFunction"}}]},{"class":"cTickFunctionObject","name":"TickFunction","guid":[1073805371,671303357,2541042835,2675230244],"parentGuid":[1374138560,3022407613,3219806122,350961622],"components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"TickFunction"}},{"id":19,"class":"sNetworkComponent","data":{}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":10,"class":"sLuaComponent","data":{"m_luaContent":"--tick脚本, 每帧会调用一次. 变量'delta'代表每帧的间隔\r\n\r\nlocal hairModule = require(HairModule)\r\n\r\nhairModule:OnLogicUpdate(delta)","m_scriptType":"kTickFunction"}}]},{"class":"cStartFunctionObject","name":"UIInit","guid":[1145104826,1897220880,3061734194,3322342058],"parentGuid":[1374138560,3022407613,