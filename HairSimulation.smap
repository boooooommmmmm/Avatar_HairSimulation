end)\r\n\r\n--[[\r\nlocal UIRoot = world.SvenUI\r\nInput.OnKeyDown:Connect(function()\t\t\r\n\tif Input.GetPressKeyData(Enum.KeyCode.UpArrow) == 1 then\r\n\t\tWind.Value =  Wind.Value + Vector3(0, 0, 1) * amplifier\r\n\t\tUIRoot.InputWindX.Text = Wind.Value.x\r\n\tend\r\n\t\r\n\tif Input.GetPressKeyData(Enum.KeyCode.DownArrow) == 1 then\r\n\t\tWind.Value =  Wind.Value + Vector3(0, 0, -1) * amplifier\r\n\tend\r\n\t\r\n\tif Input.GetPressKeyData(Enum.KeyCode.LeftArrow) == 1 then\r\n\t\tWind.Value =  Wind.Value + Vector3(-1, 0, 0) * amplifier\r\n\tend\r\n\t\r\n\tif Input.GetPressKeyData(Enum.KeyCode.RightArrow) == 1 then\r\n\t\tWind.Value =  Wind.Value + Vector3(1, 0, 0) * amplifier\r\n\tend\r\nend)\r\n--]]","m_scriptType":"kStartFunction"}}]},{"class":"cPrimitiveObject","name":"Cube","guid":[3940010526,2331264189,2837695213,2668197998],"parentGuid":[1981988479,3555894,2597800867,69741929],"components":[{"id":0,"class":"sRegularTransform","data":{"m_localPosition":[3.049424171447754,0.40000152587890627,-1.0995073318481446]}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"Cube"}},{"id":19,"class":"sNetworkComponent","data":{}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":10,"class":"sBasicShapeComponent","data":{}},{"id":12,"class":"sRigidBodyComponent","data":{"m_density":800.0,"m_restitution":0.6000000238418579,"m_statusFlag":10}},{"id":13,"class":"sMaterialComponent","data":{"m_uvScale":2.0,"m_materialType":"kLeather","m_color":[0.9430075287818909,0.6430152058601379,0.34300756454467776,0.0]}},{"id":30,"class":"sPrimitiveRenderComponent","data":{}},{"id":3,"class":"sSizeComponent","data":{}},{"id":666,"class":"sStateSyncComponent","data":{}}]},{"class":"cPrimitiveObject","name":"Cone","guid":[1059803220,3880472234,2745505871,3864541912],"parentGuid":[3940010526,2331264189,2837695213,2668197998],"components":[{"id":0,"class":"sRegularTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"Cone"}},{"id":19,"class":"sNetw